{
    "name": "enjoys/config",
    "description": "Parse config files",
    "license": "MIT",
    "authors": [
        {
            "name": "Enjoys",
            "email": "deadly.info@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "symfony/yaml": "^5.4 | ^6.0 | ^7.0",
        "psr/log": "^1.0 | ^2.0 | ^3.0",
        "ext-json": "*",
        "enjoys/functions": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "3.*",
        "vimeo/psalm": "^5.0 | ~6.4.1",
        "php-coveralls/php-coveralls": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Enjoys\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Enjoys\\Config\\": "tests/"
        }
    },
    "scripts": {
        "psalm": "./vendor/bin/psalm --show-info=true ./src"
    }
}
