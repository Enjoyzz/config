{
  "name": "enjoys/config",
  "description": "Parse config files",
  "license": "MIT",
  "authors": [
    {
      "name": "Enjoys",
      "email": "deadly.info@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "enjoys/traits": "^1.0",
    "symfony/yaml": "^5.1.0",
    "psr/log": "^1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4",
    "squizlabs/php_codesniffer": "3.*",
    "vimeo/psalm": "^4.1",
    "php-coveralls/php-coveralls": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "Enjoys\\Config\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Enjoys\\Config\\": "tests/"
    }
  },
  "scripts": {
    "psalm": "./vendor/bin/psalm --show-info=true ./src"
  }
}
